{
  "version": 2,
  "name": "proanthem-neon-fixed",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/api/signup", "destination": "/api/signup.js" },
    { "source": "/api/login", "destination": "/api/login.js" },
    { "source": "/api/songs", "destination": "/api/songs.js" },
    { "source": "/api/setlists", "destination": "/api/setlists.js" },
    { "source": "/api/admin-tasks", "destination": "/api/admin-tasks.js" },
    { "source": "/api/finances", "destination": "/api/finances.js" },
    { "source": "/api/merch", "destination": "/api/merch.js" },
    { "source": "/api/stage-plots", "destination": "/api/stage-plots.js" },
    { "source": "/signup", "destination": "/signup.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/project-anthem", "destination": "/ProjectAnthem.html" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization" }
      ]
    }
  ]
}
