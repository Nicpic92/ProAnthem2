{
  "version": 2,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/projectanthem", "destination": "/ProjectAnthem.html" },
    { "source": "/demo", "destination": "/Demo.html" },
    { "source": "/admin", "destination": "/admin.html" },
    { "source": "/login", "destination": "/proanthem_index.html" },
    { "source": "/bands/:slug", "destination": "/band-profile.html" }
  ]
}
