{
  "version": 2,
  "name": "proanthem-free",
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "proanthem_index.html", "use": "@vercel/static" },
    { "src": "dashboard.html", "use": "@vercel/static" },
    { "src": "ProjectAnthem.html", "use": "@vercel/static" },
    { "src": "Band.html", "use": "@vercel/static" },
    { "src": "stage-plot-editor.html", "use": "@vercel/static" },
    { "src": "admin.html", "use": "@vercel/static" },
    { "src": "js/**/*.js", "use": "@vercel/static" },
    { "src": "css/**/*.css", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/login", "destination": "/proanthem_index.html" },
    { "source": "/dashboard", "destination": "/dashboard.html" },
    { "source": "/editor", "destination": "/ProjectAnthem.html" },
    { "source": "/band", "destination": "/Band.html" },
    { "source": "/stage-plot", "destination": "/stage-plot-editor.html" },
    { "source": "/admin", "destination": "/admin.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ]
}
